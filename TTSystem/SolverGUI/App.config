<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ITTServ" />
            </basicHttpBinding>
            <wsDualHttpBinding>
                <binding name="DualEndpoint">
                    <security mode="None" />
                </binding>
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/TTService/TTSolverSvc"
                binding="wsDualHttpBinding" bindingConfiguration="DualEndpoint"
                contract="TTSolverSvcClient.ITTSolverSvc" name="DualEndpoint" />
            <endpoint address="http://localhost:63756/Service.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_ITTServ" contract="TTSvcClient.ITTServ"
                name="BasicHttpBinding_ITTServ" />
        </client>
    </system.serviceModel>
</configuration>